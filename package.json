{
  "name": "Playground",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios --simulator='iPhone 11 Pro Max'",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx && prettier --check ./**/*.{ts,tsx}",
    "lint:fix": "eslint . --ext .js,.jsx,.ts,.tsx --fix && prettier --write .",
    "start": "react-native start --reset-cache",
    "prepare": "husky install",
    "pre-commit": "yarn tsc && yarn lint",
    "test": "jest",
    "pod": "cd ios && pod install && cd ..",
    "clean": "cd android && ./gradlew clean && cd .."
  },
  "dependencies": {
    "@react-native-async-storage/async-storage": "^1.17.11",
    "@react-native-clipboard/clipboard": "^1.11.2",
    "@react-native-community/checkbox": "^0.5.15",
    "@react-native-cookies/cookies": "^6.2.0",
    "@react-navigation/bottom-tabs": "^6.5.7",
    "@react-navigation/native": "^6.1.6",
    "@react-navigation/native-stack": "^6.9.12",
    "@reduxjs/toolkit": "^1.9.3",
    "@shopify/flash-list": "^1.5.0",
    "@shopify/react-native-skia": "^1.2.0",
    "@shopify/restyle": "^2.4.1",
    "@tanstack/react-query": "^4.26.1",
    "axios": "^1.6.2",
    "dayjs": "^1.11.8",
    "lottie-react-native": "^6.4.0",
    "moment": "^2.29.4",
    "moti": "^0.26.1",
    "react": "18.2.0",
    "react-native": "0.72.6",
    "react-native-camera": "^4.2.1",
    "react-native-gesture-handler": "^2.13.4",
    "react-native-image-colors": "^2.4.0",
    "react-native-keyboard-aware-scroll-view": "^0.9.5",
    "react-native-linear-gradient": "^2.6.2",
    "react-native-modal": "^13.0.1",
    "react-native-redash": "^18.1.1",
    "react-native-scaled-sheet": "^0.9.0",
    "react-native-size-matters": "^0.4.0",
    "react-native-svg": "^13.8.0",
    "react-native-system-navigation-bar": "^2.6.1",
    "react-native-tab-view": "^3.5.2",
    "react-native-user-agent": "^2.3.1",
    "react-native-user-inactivity": "^1.2.0",
    "react-native-vector-icons": "^9.2.0",
    "react-native-view-shot": "^3.7.0",
    "react-native-vision-camera": "^3.8.2",
    "react-redux": "^8.1.0",
    "redux-persist": "^6.0.0",
    "uuid": "^9.0.0",
    "zxcvbn": "^4.4.2"
  },
  "devDependencies": {
    "@babel/core": "^7.20.0",
    "@babel/preset-env": "^7.20.0",
    "@babel/runtime": "^7.20.0",
    "@react-native-community/eslint-config": "^3.2.0",
    "@tsconfig/react-native": "^2.0.2",
    "@types/jest": "^29.2.1",
    "@types/react": "^18.0.24",
    "@types/react-native": "^0.70.6",
    "@types/react-native-vector-icons": "^6.4.13",
    "@types/react-test-renderer": "^18.0.0",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "babel-jest": "^29.2.1",
    "babel-plugin-module-resolver": "^5.0.0",
    "eslint": "^8.19.0",
    "husky": "^8.0.3",
    "jest": "^29.2.1",
    "lint-staged": "^13.2.3",
    "metro-react-native-babel-preset": "0.73.10",
    "prettier": "^2.4.1",
    "react-native-asset": "^2.1.0",
    "react-native-reanimated": "^3.0.0",
    "react-native-svg-transformer": "^1.0.0",
    "react-test-renderer": "18.2.0",
    "typescript": "4.8.4"
  },
  "engines": {
    "node": ">=16"
  },
  "lint-staged": {
    "**/*.{js,jsx,ts,tsx}": [
      "yarn eslint",
      "yarn prettier --write"
    ]
  },
  "jest": {
    "preset": "react-native"
  }
}
